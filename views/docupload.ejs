<%- include('includes/navbar.ejs') %>
<link rel="stylesheet" href="css/docupload.css">
<div class="flex-container">
    <div class="student-profile py-4 flex-item-1">
           <!-- <div class="container"> -->
             <!-- <div class="row"> -->
               <!-- <div class="col-lg-4"> -->
                <form action="#" method="post">
                 <div class="card shadow-sm" style="background-color: #FBE3E3;">
                   <div class="card-header bg-transparent text-center" style="border-bottom-width: 2px; border-color:#B20600;
   ">
                     <img class="profile_img" src="images/profile_img.png" alt="profile icon">
                     <h3>Student Info.</h3>
                   </div>
                   <div class="card-body">
                    <p class="mb-0"><strong class="pr-1">Name:</strong><%= st_name %></p>
                    <p class="mb-0"><strong class="pr-1">Student ID:</strong><%= st_id %></p>
                    <p class="mb-0"><strong class="pr-1">Department:</strong><%= st_dept %></p>
                    <p class="mb-0"><strong class="pr-1">Contact:</strong><%= st_contact %></p>
                    <p class="mb-0"><strong class="pr-1">Mail:</strong><%= st_mail %></p>
                    <center><input class="submit-button" type="submit" value="submit"></center>
                   </div>
     </div>  <!-- <div class="bw"></div> -->
   </div>
     <div class="rightbox">
       <div class="box">
         <div class="a"><img class="innerimg" src="images/id.png">
           <label class="img inBox">Upload Image</label>
           <input type="file" id="student_image" class="img inBox" value="student_image" name="student_image">
         </div>
         <div class="a"><img class="innerimg sign" src="images/sign.png">
           <label class="sig inBox">Upload Signature</label>
           <input type="file" class="sig inBox" value="student_signature" name="student_signature">
   
         </div>
         <div class="a"><img class="innerimg" src="images/pdf.png">
           <label class="project inBox">Upload project synopsis</label>
           <input type="file" class="project inBox" value="student_synopsis" name="student_synopsis">
         </div>
         <div class="a"><img class="innerimg" src="images/pdf.png">
           <label class="pdf inBox">Upload file</label>
           <input type="file" title=" " class="pdf inBox" value="student_file1" name="student_file1" />
         </div>
         <div class="a"><img class="innerimg" src="images/pdf.png">
           <label class="pdf inBox">Upload file</label>
           <input type="file" class="pdf inBox" value="student_file2" name="student_file2"/>
         </div>
        </form>
       </div>
   
   </div>
   </div>
   
   
   
   
   
   
   
   
   
   
   
   
   
   
         
   
   
   
   
   
   
